#!/bin/sh

PJPORT=5280
# SRV record example:
#PJSRVHOST="_xampp._tcp.example.org,example.org,5222,0,100"
PJSRVHOST=""

# Do not modify below

if [ -z "$PJSRVHOST" ]
then
  echo docker run -p $PJPORT:5280 -d -dns 127.0.0.1 -name punjab thisgeek/punjab
else
  echo docker run -p $PJPORT:5280 -d -e "PJSRVHOST=$PJSRVHOST" -dns 127.0.0.1 -name punjab thisgeek/punjab
fi
